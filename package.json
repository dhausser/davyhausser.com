{
  "name": "gatsbygram",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Davy Hausser <davy.hausser@gmail.com>",
  "dependencies": {
    "@chakra-ui/core": "^0.7.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@stripe/stripe-js": "^1.3.2",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.20.12",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-chakra-ui": "^0.1.4",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-json": "^2.3.1",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.4",
    "isomorphic-unfetch": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "homepage": "https://davyhausser.com",
  "keywords": [
    "gatsby",
    "davyhausser"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "engines": {
    "node": ">=13.12.0"
  },
  "eslintIgnore": [
    "interfaces",
    "**/__tests__/fixtures/"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --ext .js,.jsx,.ts,.tsx --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged || node scripts/on-lint-error.js"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "netlify dev",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {}
}